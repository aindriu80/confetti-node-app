
<div class="container">
  <div class="centered">
    <h2>Users Table</h2>
  </div>
<table class="responsive-table striped centered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Eircode</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td>
        <%= user.fullName %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.eirCode %>
      </td>
    </tr>
    <% }); %>
  </tbody>  
</table>
</div>